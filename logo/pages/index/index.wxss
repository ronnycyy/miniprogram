.custom {
  position: fixed;
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  background-color: rgb(53, 26, 175);
  color: #FFFFFF;
}

.custom>image {
  width: 200rpx;
  height: 150rpx;
}

.body {
  display: block;
  width: 100%;
  height: 100vh; 
  color: white;
  background-color: black;
  overflow-y: auto;
}
